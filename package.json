{
  "name": "typescript-vm",
  "version": "1.0.0",
  "description": "A TypeScript virtual machine with custom language support",
  "main": "dist/index.js",
  "bin": {
    "tsvm": "./bin/tsvm"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:cli": "jest --testPathPattern=cli",
    "test:performance": "ts-node src/testing/performance-benchmarks.ts",
    "test:regression": "ts-node src/testing/regression-tests.ts",
    "benchmark": "ts-node src/testing/performance/run-benchmarks.ts",
    "benchmark:individual": "ts-node src/testing/performance/run-benchmarks.ts --individual",
    "benchmark:memory": "ts-node src/testing/performance/memory-profiler.ts",
    "benchmark:cpu": "ts-node src/testing/performance/cpu-profiler.ts",
    "benchmark:advanced": "ts-node src/testing/performance/benchmark-suite.ts",
    "test:toolchain": "ts-node scripts/test-toolchain.ts",
    "test:all": "npm test && npm run test:integration && npm run test:cli && npm run test:toolchain",
    "dev": "ts-node src/index.ts",
    "repl": "ts-node src/tools/repl.ts",
    "cli": "ts-node src/cli/index.ts",
    "start": "npm run cli",
    "compile": "ts-node src/cli/index.ts --compile",
    "run": "ts-node src/cli/index.ts --run",
    "debug": "ts-node src/cli/index.ts --debug",
    "disassemble": "ts-node src/cli/index.ts --disassemble",
    "assemble": "ts-node src/cli/index.ts --assemble",
    "demo": "ts-node scripts/demo-workflow.ts"
  },
  "keywords": ["vm", "virtual-machine", "compiler", "interpreter", "typescript"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "readline": "^1.3.0"
  }
}